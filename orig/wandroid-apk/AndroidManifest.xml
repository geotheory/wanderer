<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:a="http://schemas.android.com/apk/res/android" a:versionCode="11" a:versionName="1.0.11" package="de.agrothe.wanderer">
    <supports-screens a:anyDensity="true" a:largeScreens="true" a:normalScreens="true" a:smallScreens="true"/>
    <application a:description="@string/app_description" a:icon="@drawable/activity_icon" a:label="@string/app_name" a:theme="@style/Theme.NoBackground">
        <activity a:name=".MainActivity" a:screenOrientation="landscape">
            <intent-filter>
                <action a:name="android.intent.action.MAIN"/>
                <category a:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>